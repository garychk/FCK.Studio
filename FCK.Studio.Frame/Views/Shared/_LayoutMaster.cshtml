@using FCK.Studio.Frame.Core
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <link rel="icon" type="image/png" href="/Content/favicon.png">
    <link rel="stylesheet" href="/Content/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="/Content/fullcalendar/fullcalendar.css" />
    <link rel="stylesheet" href="/Content/jquery-confirm.css" />
    <link rel="stylesheet" href="//at.alicdn.com/t/font_rd0pe6mihuboi529.css" />
    <link rel="stylesheet" href="/Content/app.css" />
    <script src="/Content/jquery.min.js"></script>
    <script src="/Content/fullcalendar/lib/moment.min.js"></script>
    <script src="/Content/fullcalendar/lib/jquery-ui.custom.min.js"></script>
    <script src="/Content/bootstrap/js/bootstrap.min.js"></script>
    <script src="/Content/fullcalendar/fullcalendar.min.js"></script>
    <script src="/Content/jquery-confirm.js"></script>
    <script src="/Content/jquery.form.js"></script>
    <script src="/Content/app.js"></script>
    <script>
        $(function () {
            var reg = new Register();
            reg.GetModelByNumber("FormGetRegister");
            $("#RegisterName").text(reg.datas.Register_Name);
        });
    </script>
</head>
<body class="app-main">
    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container-fluid">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="/Home/Dashboard">
                    <i class="iconfont icon-goback"></i>
                </a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav">
                    <li class="active"><a href="/Home/Dashboard"><i class="glyphicon glyphicon-home"></i>控制台</a></li>
                    <li><a href="/Home/Calendar"><i class="glyphicon glyphicon-calendar"></i>事务</a></li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="glyphicon glyphicon-volume-up"></i>消息 <span class="label label-danger">10</span> <span class="caret"></span></a>
                        <ul class="dropdown-menu" role="menu">
                            <li><a href="#">Action</a></li>
                            <li><a href="#">Another action <span class="label label-success">3</span></a></li>
                            <li><a href="#">Something else here <span class="label label-danger">1</span></a></li>
                            <li class="divider"></li>
                            <li><a href="#">Separated link eparated link <span class="label label-warning">6</span></a></li>
                            <li class="divider"></li>
                            <li><a href="#">One more separated link</a></li>
                        </ul>
                    </li>
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="glyphicon glyphicon-user"></i>@(CookieHelper.CookieVal("UserName")) <span class="caret"></span></a>
                        <ul class="dropdown-menu" role="menu">
                            <li><a href="#"><i class="glyphicon glyphicon-th-large"></i>个人设置</a></li>
                            <li><a href="#"><i class="glyphicon glyphicon-lock"></i>安全设置</a></li>
                            <li class="divider"></li>
                            <li><a href="/Home/"><i class="glyphicon glyphicon-off"></i>退出</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container-fluid -->
    </nav>
    <div class="container-fluid">
        @RenderBody()
        <p class="text-center text-block">
            <small>Copyright &copy; <b id="RegisterName">FCK.Studio</b> @(DateTime.Now.ToString("yyyy-MM"))</small>
        </p>
    </div>
    <form id="FormGetRegister" class="hide"></form>
</body>
</html>
