@model FCK.Studio.Core.PageDatas<FCK.Studio.Core.ArticleDto>
@{
    ViewBag.Title = "关于我们——时优谷无锡网页设计第一品牌|自适应网页设计|自适应网站开发|HTML5网页制作|Bootstrap网站|网站优化";
    ViewBag.description = "时优谷（GOOSTUDIO）是一家专注于HTML5网站建设，微信开发，APP定制开发，以及SEO优化和网站推广的创新设计工作室。工作室成立于 2014年初，在网页设计和互联网应用开发领域有着独特理解。";
    ViewBag.keywords = "自适应网页设计|自适应网站开发|HTML5网页制作|Bootstrap网站|网站优化|无锡微信开发|无锡APP开发";
    Layout = "~/Views/Shared/_LayoutMaster.cshtml";
}
<aside class="fh5co-page-heading">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h1 class="fh5co-page-heading-lead">About Us
						<span class="fh5co-border"></span>
                </h1>

            </div>
        </div>
    </div>
</aside>

<div id="fh5co-main">

    <div class="container">
        <div class="row">
            <div class="col-md-8">
                <h2>我们是谁？</h2>
                <p>
                    时优谷（GOOSTUDIO）是一家专注于HTML5网站建设，微信开发，APP定制开发，以及SEO优化和网站推广的创新设计工作室。工作室成立于 2014年初，在网页设计和互联网应用开发领域有着独特理解。

工作室聚集了多名专业互联网开发工程师，网页交互设计师，团队成员均有八年以上的项目经验，能够准确把握客户需求，为客户快速搭建互联网上的宣传平台。

我们一直专注于互联网应用领域的开发，除了为客户搭建网站，还能够为客户开发基于浏览器的互动界面，开发手机应用软件界面，以及微信应用开发，团队整体有着前瞻性的互联网思想；以精湛的技艺和热诚的服务，洞察互联网发展趋势。建立更好的网络形象与口碑，把客户的市场营销真正的做到互联网，并且实现互联网营销的梦想是时优谷（GOOSTUDIO）团队成立的本意！
                </p>
                <div class="row">
                    <div class="col-md-6">
                        <p><a href="/Content/themes/IT/images/banner_3.jpg" class="image-popup">
                            <img src="/Content/themes/IT/images/banner_3.jpg" alt="GooStudio" class="img-responsive img-rounded"></a></p>
                    </div>
                    <div class="col-md-6">
                        <p><a href="/Content/themes/IT/images/banner_4.jpg" class="image-popup">
                            <img src="/Content/themes/IT/images/banner_4.jpg" alt="GooStudio" class="img-responsive img-rounded"></a></p>
                    </div>
                </div>
                <div class="fh5co-spacer fh5co-spacer-xxs"></div>
                <div class="row">
                    <div class="col-md-6">
                        <p>我们的优势：</p>
                        <ul class="fh5co-list-check">
                            <li>9年设计经验UI设计师</li>
                            <li>8年工作经验程序员</li>
                            <li>PC、微信、APP一站式服务</li>
                            <li>精通.NET\PHP\JAVA\Jquery前沿技术</li>
                            <li>7X24小时售后服务</li>
                        </ul>
                    </div>
                    <div class="col-md-6">
                        <p>常见问题：</p>
                        <ul class="fh5co-list-check">
                            @foreach(var item in Model.datas){
                            <li><a href="/Home/Detail/@item.Article_ID">@item.Article_Title</a></li>
                            }
                        </ul>
                    </div>
                </div>

            </div>

            <div class="col-md-4">
                @Html.Action("_PartialSide", "Home", new { page = 1, pageSize = 10, catename = "新闻", orderindex = "timedesc", isrec = 0, ispage = 0 })
            </div>

        </div>
    </div>
</div>

<div class="fh5co-spacer fh5co-spacer-lg"></div>
